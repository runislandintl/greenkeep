{
  "name": "greenkeep",
  "version": "0.1.0",
  "private": true,
  "description": "GreenKeep - SaaS multi-tenant golf course greenkeeping management",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "cd server && node src/index.js",
    "dev:client": "cd client && npx vite",
    "build": "cd client && npx vite build",
    "test:server": "cd server && npx vitest run",
    "test:client": "cd client && npx vitest run",
    "seed": "cd server && node src/seed.js"
  },
